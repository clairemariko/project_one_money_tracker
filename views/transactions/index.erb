<h1>Transactions</h1>

<table>

  <thead>
    <th>Date</th>
    <th>Merchant</th>
    <th>Tag</th>
    <th>Value</th>
    
  </thead>

  <tbody>

    <% @transactions.each do |transaction| %>
      <tr>
        <td><%= transaction.transaction_date %></td>
        <td><%= transaction.merchant.name %></td>
        <td><%= transaction.tag.name %></td>
        <td><%= transaction.value %></td>
      </tr>
  <% end %>
  </tbody>
</table>
